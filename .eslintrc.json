{
    "extends": [
        "airbnb"
    ],
    "plugins": ["jest"],
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jest/globals": true
    },
    "rules": {
        "strict": 0,
        "no-tabs": 0,
        "indent": [2, "tab", {
            "SwitchCase": 1
        }],
        "comma-dangle": [2, "never"],
        "prefer-destructuring": 0,
        "jsx-quotes": [1, "prefer-single"],
        "max-len": [1, {
            "code": 80,
            "ignoreRegExpLiterals": true
        }],
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }],
        "react/jsx-indent": [1, "tab"],
        "react/jsx-indent-props": [1, "tab"],
        "react/prop-types": 0
    }
}